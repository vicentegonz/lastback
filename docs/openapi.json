  "components": {
    "securitySchemes": {
      "JSONWebTokenAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      },
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-Api-Key"
      }
    },
    "responses": {
      "UnauthorizedError": {
        "description": "Unauthorized",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "detail": {
                  "type": "string",
                  "enum": ["Authentication credentials were not provided."]
                }
              }
            }
          }
        }
      },
      "StoreNotFound": {
        "description": "Store not found",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "detail": {
                  "type": "string",
                  "enum": ["Not found."]
                }
              }
            }
          }
        }
      },
      "ForecastError": {
        "description": "Bad Gateway: Invalid parameters for Forecast",
        "content": {
          "application/json": {
            "schema": {
